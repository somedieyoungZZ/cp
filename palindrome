#include<bits/stdc++.h>

using namespace std;


int main () {

    string str;
    getline(cin, str);
    string str_rev;

    for(int i = str.size() - 1; i >= 0; --i) {
        str_rev.push_back(str[i]);
    }

    if(str == str_rev){
    cout << "yes it is palindrome";
    } else {
    cout << "no not a palindrome";
    }


    return 0;
}
